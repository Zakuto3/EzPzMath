<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.waki.ezpzmath">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ezpzmath_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ezpzmath_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service android:name=".MusicService" android:enabled="true" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LevelsActivity"
            android:parentActivityName=".ModesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ModesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ScoreActivity"
            android:parentActivityName=".ModesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SettingsActivity"
            android:parentActivityName=".ModesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GameActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HowToPlayActivity"
            android:screenOrientation="portrait"></activity>
        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true"/>
        <meta-data
            android:name="ca-app-pub-4962417284462920~5794185725"
            android:value="ca-app-pub-4962417284462920/7834805951"/>

    </application>

</manifest>